{
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "manager.html",
        "default_title": "uTorrent easy client"
    },
    "background": {
        "scripts": ["js/mono.js","js/lang.js","js/background.js"]
    },
    "icons": {
        "128": "images/icon_128.png"
    },
    "permissions": [ "http://*/*", "https://*/*", "storage", "notifications", "tabs", "contextMenus" ],
    "name": "uTorrent easy client",
    "description": "__MSG_appDesc__",
    "default_locale": "en",
    "options_page": "options.html",
    "version": "2.2.57"
}